{
  "manifest_version": 2,
  "name": "bukmarkz",
  "description": "a chrome extension to view your bookmarks. (built with angularJS)",
  "version": "0.0.1",
  "icons": {
	 "16": "img/icon/icon16.png",
	 "48": "img/icon/icon48.png",
	 "128": "img/icon/icon128.png"
  },
  "browser_action": {
    "default_icon": {
		"19": "img/icon/icon19.png",
		"38": "img/icon/icon38.png"
	},
	"default_title": "bukmarkz",
    "default_popup": "app/app.html"
  },
  "permissions": [
    "tabs"
  ]
}